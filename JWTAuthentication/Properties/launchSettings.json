    "JWTAuthentication": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "login",
      "applicationUrl": "http://0.0.0.0:5000;https://0.0.0.0:5001", //only for local
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
